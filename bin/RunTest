./autobot --verbose --connector=saucelabs:rmiguel:win7_chrome34 --env=qa -t registration &
./autobot --verbose --connector=saucelabs:rmiguel:win7_chrome34 --env=qa -t search &
./autobot --verbose --connector=saucelabs:rmiguel:win7_chrome34 --env=qa -t seo_links &
./autobot --verbose --connector=saucelabs:rmiguel:win7_chrome34 --env=qa -t sign_in

