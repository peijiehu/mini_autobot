./autobot --verbose --connector=saucelabs:rmiguel:win7_ie11 --env=qa -t registration &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ie11 --env=qa -t search &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ie11 --env=qa -t seo_links &
