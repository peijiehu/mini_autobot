./autobot --verbose --connector=saucelabs:rmiguel:win7_ff29 --env=qa -t pdp &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ff29 --env=qa -t registration &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ff29 --env=qa -t search &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ff29 --env=qa -t seo_links &
./autobot --verbose --connector=saucelabs:rmiguel:win7_ff29 --env=qa -t sign_in

wait
echo "All complete for ff29"

